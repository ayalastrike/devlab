LOWER	calcTemp.c	3;"	d	file:
STEP	calcTemp.c	5;"	d	file:
UPPER	calcTemp.c	4;"	d	file:
main	calcTemp.c	/^int main() {$/;"	f
main	lineCnt.c	/^int main() {$/;"	f
main	testIOChar.c	/^int main() {$/;"	f
main	wordCnt.c	/^int main () {$/;"	f
